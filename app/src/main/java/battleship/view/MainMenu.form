<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="battleship.view.MainMenu">
  <grid id="27dc6" binding="mainPanel" layout-manager="GridBagLayout">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="7f163" class="javax.swing.JLabel" binding="gameName">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag weightx="1.0" weighty="1.0"/>
        </constraints>
        <properties>
          <text resource-bundle="ui_strings" key="gameName"/>
        </properties>
      </component>
      <component id="81a33" class="javax.swing.JButton" binding="hostButton">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag top="0" left="100" bottom="0" right="100" weightx="1.0" weighty="1.0" ipady="20"/>
        </constraints>
        <properties>
          <text resource-bundle="ui_strings" key="host"/>
        </properties>
      </component>
      <component id="6687" class="javax.swing.JButton" binding="joinButton">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag top="0" left="100" bottom="0" right="100" weightx="1.0" weighty="1.0" ipady="20"/>
        </constraints>
        <properties>
          <text resource-bundle="ui_strings" key="join"/>
        </properties>
      </component>
      <component id="6dd87" class="javax.swing.JButton" binding="exitButton">
        <constraints>
          <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag top="0" left="100" bottom="0" right="100" weightx="1.0" weighty="1.0" ipady="20"/>
        </constraints>
        <properties>
          <text resource-bundle="ui_strings" key="exit"/>
        </properties>
      </component>
      <component id="44bd" class="javax.swing.JButton" binding="playButton" default-binding="true">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag top="0" left="100" bottom="0" right="100" weightx="1.0" weighty="1.0" ipady="20"/>
        </constraints>
        <properties>
          <enabled value="false"/>
          <text resource-bundle="ui_strings" key="play"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
